<!--pages/login/login.wxml-->
<view class="v1">
    <view class="v2">
      <view  class="dltext" style="width: 252rpx; height: 89rpx; display: block; box-sizing: border-box; left: 0rpx; top: -2rpx">修改密码</view>
      

      <view class="passwordCs">
        <image src="/img/密码.png" class="ps"></image>
        <input bindinput="input2" placeholder="请输入密码" type="password"/>
      </view>

      <view class="passwordCs1">
        <image src="/img/密码.png" class="ps"></image>
        <input bindinput="input3" placeholder="请确认密码" type="password"/>
      </view>


      <view class="login">
        <button class="btn-dl" type="primary" bindtap="register">确认修改</button>
      </view>
    </view>

    
</view>